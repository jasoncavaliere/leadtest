@{

	// View
	View = this;
	SetClientVar("login", LoginStatus);

	// Global Page Rendering event
	Page_Rendering();

	// Page Render event
	LeadStatus_List.Page_Render();

	// Check action result
	if (LeadStatus_List.ActionResult is EmptyResult) { // Terminate page
		return;
	}

	// Layout
	Layout = "_Layout";

	// Local variables (Temp data)
	#pragma warning disable 168
	string sqlWrk, filterWrk, whereWrk, curVal;
	List<Dictionary<string, object>> rswrk;
	string[] arWrk;
	List<object> listWrk;
	Func<string> lookupFilter;
	#pragma warning restore 168
}
@if (!LeadStatus_List.IsExport()) {
<script>
var fLeadStatuslist, currentPageID;
loadjs.ready("head", function() {

	// Form object
	currentPageID = ew.PAGE_ID = "list";
	fLeadStatuslist = currentForm = new ew.Form("fLeadStatuslist", "list");
	fLeadStatuslist.formKeyCountName = '@(LeadStatus_List.FormKeyCountName)';
	loadjs.done("fLeadStatuslist");
});
</script>
<script>
loadjs.ready("head", function() {

	// Client script
	// Write your client script here, no need to add script tags.

});
</script>
}
@if (!LeadStatus_List.IsExport()) {
<div class="btn-toolbar ew-toolbar">
@if (LeadStatus_List.TotalRecords > 0 && LeadStatus_List.ExportOptions.Visible) {
	LeadStatus_List.ExportOptions.Render("body");
}
@if (LeadStatus_List.ImportOptions.Visible) {
	LeadStatus_List.ImportOptions.Render("body");
}
<div class="clearfix"></div>
</div>
}
@{ LeadStatus_List.RenderOtherOptions(); }
@LeadStatus_List.ShowPageHeader()
@LeadStatus_List.ShowMessages()
@if (LeadStatus_List.TotalRecords > 0 || !Empty(LeadStatus.CurrentAction)) {
<div class="card ew-card ew-grid@(LeadStatus_List.IsAddOrEdit ? " ew-grid-add-edit" : "") LeadStatus">
<form name="fLeadStatuslist" id="fLeadStatuslist" class="form-inline ew-form ew-list-form" action="@AppPath(CurrentPageName())" method="post">
@if (LeadStatus_List.CheckToken) {
	@Html.AntiForgeryToken()
}
<div id="gmp_LeadStatus" class="@ResponsiveTableClass() card-body ew-grid-middle-panel">
@if (LeadStatus_List.TotalRecords > 0 || LeadStatus_List.IsGridEdit) {
<table id="tbl_LeadStatuslist" class="table ew-table"><!-- .ew-table -->
<thead>
	<tr class="ew-table-header">
@{

	// Header row
	LeadStatus.RowType = Config.RowTypeHeader;

	// Render list options
	await LeadStatus_List.RenderListOptions();

	// Render list options (header, left)
	LeadStatus_List.ListOptions.Render("header", "left");
}
@if (LeadStatus_List.Id.Visible) { // LeadStatus_List.Id.Visible (begin)
	if (LeadStatus_List.SortUrl(LeadStatus_List.Id) == "") {
		<th data-name="Id" class="@(LeadStatus_List.Id.HeaderCellClass)"><div id="elh_LeadStatus_Id" class="LeadStatus_Id"><div class="ew-table-header-caption">@Html.Raw(LeadStatus_List.Id.Caption)</div></div></th>
	} else {
		<th data-name="Id" class="@(LeadStatus_List.Id.HeaderCellClass)"><div class="ew-pointer" onclick="ew.sort(event, '@Html.Raw(LeadStatus_List.SortUrl(LeadStatus_List.Id))', 1);"><div id="elh_LeadStatus_Id" class="LeadStatus_Id">
			<div class="ew-table-header-btn"><span class="ew-table-header-caption">@Html.Raw(LeadStatus_List.Id.Caption)</span><span class="ew-table-header-sort">@if (LeadStatus_List.Id.Sort == "ASC") {<i class="fas fa-sort-up"></i>} else if (LeadStatus_List.Id.Sort == "DESC") {<i class="fas fa-sort-down"></i>}</span></div>
		</div></div></th>
	}
} @* LeadStatus_List.Id.Visible (end) *@
@if (LeadStatus_List.DisplayName.Visible) { // LeadStatus_List.DisplayName.Visible (begin)
	if (LeadStatus_List.SortUrl(LeadStatus_List.DisplayName) == "") {
		<th data-name="DisplayName" class="@(LeadStatus_List.DisplayName.HeaderCellClass)"><div id="elh_LeadStatus_DisplayName" class="LeadStatus_DisplayName"><div class="ew-table-header-caption">@Html.Raw(LeadStatus_List.DisplayName.Caption)</div></div></th>
	} else {
		<th data-name="DisplayName" class="@(LeadStatus_List.DisplayName.HeaderCellClass)"><div class="ew-pointer" onclick="ew.sort(event, '@Html.Raw(LeadStatus_List.SortUrl(LeadStatus_List.DisplayName))', 1);"><div id="elh_LeadStatus_DisplayName" class="LeadStatus_DisplayName">
			<div class="ew-table-header-btn"><span class="ew-table-header-caption">@Html.Raw(LeadStatus_List.DisplayName.Caption)</span><span class="ew-table-header-sort">@if (LeadStatus_List.DisplayName.Sort == "ASC") {<i class="fas fa-sort-up"></i>} else if (LeadStatus_List.DisplayName.Sort == "DESC") {<i class="fas fa-sort-down"></i>}</span></div>
		</div></div></th>
	}
} @* LeadStatus_List.DisplayName.Visible (end) *@
@{

	// Render list options (header, right)
	LeadStatus_List.ListOptions.Render("header", "right");
}
	</tr>
</thead>
<tbody>
@{ // Begin Code
if (LeadStatus_List.ExportAll && LeadStatus_List.IsExport()) {
	LeadStatus_List.StopRecord = LeadStatus_List.TotalRecords;
} else {

	// Set the last record to display
	if (LeadStatus_List.TotalRecords > LeadStatus_List.StartRecord + LeadStatus_List.DisplayRecords - 1) {
		LeadStatus_List.StopRecord = LeadStatus_List.StartRecord + LeadStatus_List.DisplayRecords - 1;
	} else {
		LeadStatus_List.StopRecord = LeadStatus_List.TotalRecords;
	}
}
if (LeadStatus_List.Recordset != null && LeadStatus_List.Recordset.HasRows) {
	if (!LeadStatus_List.Connection.SelectOffset) { // DN
		for (int i = 1; i <= LeadStatus_List.StartRecord - 1; i++) { // Move to first record
			if (await LeadStatus_List.Recordset.ReadAsync()) {
				LeadStatus_List.RecordCount++;
			}
		}
	} else {
		LeadStatus_List.RecordCount = LeadStatus_List.StartRecord - 1;
	}
} else if (!LeadStatus_List.AllowAddDeleteRow && LeadStatus_List.StopRecord == 0) {
	LeadStatus_List.StopRecord = LeadStatus_List.GridAddRowCount;
}

// Initialize aggregate
LeadStatus.RowType = Config.RowTypeAggregateInit;
LeadStatus_List.ResetAttributes();
await LeadStatus_List.RenderRow();

// Output data rows
bool eof = (LeadStatus_List.Recordset == null); // DN
while (LeadStatus_List.RecordCount < LeadStatus_List.StopRecord) { // Begin While
	if (!LeadStatus_List.IsGridAdd && !eof) { // DN
		eof = !await LeadStatus_List.Recordset.ReadAsync();
	}
	LeadStatus_List.RecordCount++;
	if (LeadStatus_List.RecordCount >= LeadStatus_List.StartRecord) {
		LeadStatus_List.RowCount++;

		// Set up key count
		LeadStatus_List.KeyCount = ConvertToInt(LeadStatus_List.RowIndex);

		// Init row class and style
		LeadStatus_List.ResetAttributes();
		LeadStatus_List.CssClass = "";
		if (LeadStatus_List.IsGridAdd) {
		} else {
			await LeadStatus_List.LoadRowValues(LeadStatus_List.Recordset); // Load row values
		}
		LeadStatus.RowType = Config.RowTypeView; // Render view

		// Set up row id / data-rowindex
		LeadStatus_List.RowAttrs.Add("data-rowindex", Convert.ToString(LeadStatus_List.RowCount));
		LeadStatus_List.RowAttrs.Add("id", "r" + Convert.ToString(LeadStatus_List.RowCount) + "_LeadStatus");
		LeadStatus_List.RowAttrs.Add("data-rowtype", Convert.ToString(LeadStatus_List.RowType));

		// Render row
		await LeadStatus_List.RenderRow();

		// Render list options
		await LeadStatus_List.RenderListOptions();
	@:<tr @Html.Raw(LeadStatus.RowAttributes)>
	@* Render list options (body, left) *@
	@LeadStatus_List.ListOptions.RenderBody("left", LeadStatus_List.RowCount)
	if (LeadStatus_List.Id.Visible) { // LeadStatus_List.Id.Visible (begin)
		<td data-name="Id" @Html.Raw(LeadStatus_List.Id.CellAttributes)>
<span id="el@(LeadStatus_List.RowCount)_LeadStatus_Id">
<span @Html.Raw(LeadStatus_List.Id.ViewAttributes)>@Html.Raw(LeadStatus_List.Id.GetViewValue())</span>
</span>
</td>
	} @* LeadStatus_List.Id.Visible (end) *@
	if (LeadStatus_List.DisplayName.Visible) { // LeadStatus_List.DisplayName.Visible (begin)
		<td data-name="DisplayName" @Html.Raw(LeadStatus_List.DisplayName.CellAttributes)>
<span id="el@(LeadStatus_List.RowCount)_LeadStatus_DisplayName">
<span @Html.Raw(LeadStatus_List.DisplayName.ViewAttributes)>@Html.Raw(LeadStatus_List.DisplayName.GetViewValue())</span>
</span>
</td>
	} @* LeadStatus_List.DisplayName.Visible (end) *@
	@* Render list options (body, right) *@
	@LeadStatus_List.ListOptions.RenderBody("right", LeadStatus_List.RowCount)
	@:</tr>
	} @* End if *@
} @* End While *@
} @* End Code *@
</tbody>
</table><!-- /.ew-table -->
} @* End If TotalRecords > 0 *@
</div><!-- /.ew-grid-middle-panel -->
@if (Empty(LeadStatus.CurrentAction)) {
<input type="hidden" name="action" id="action" value="">
}
</form><!-- /.ew-list-form -->
@{ LeadStatus_List.CloseRecordset(); }
@if (!LeadStatus_List.IsExport()) {
<div class="card-footer ew-grid-lower-panel">
@if (!LeadStatus_List.IsGridAdd) {
<form name="ew-pager-form" class="form-inline ew-form ew-pager-form" action="@AppPath(CurrentPageName())">
@Html.Raw(LeadStatus_List.Pager.Render())
</form>
}
<div class="ew-list-other-options">
@LeadStatus_List.OtherOptions.RenderBody("bottom")
</div>
<div class="clearfix"></div>
</div>
}
</div><!-- /.ew-grid -->
}
