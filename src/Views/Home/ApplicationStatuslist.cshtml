@{ // Begin Code
if (ApplicationStatus_List.ExportAll && ApplicationStatus_List.IsExport()) {
	ApplicationStatus_List.StopRecord = ApplicationStatus_List.TotalRecords;
} else {

	// Set the last record to display
	if (ApplicationStatus_List.TotalRecords > ApplicationStatus_List.StartRecord + ApplicationStatus_List.DisplayRecords - 1) {
		ApplicationStatus_List.StopRecord = ApplicationStatus_List.StartRecord + ApplicationStatus_List.DisplayRecords - 1;
	} else {
		ApplicationStatus_List.StopRecord = ApplicationStatus_List.TotalRecords;
	}
}
if (ApplicationStatus_List.Recordset != null && ApplicationStatus_List.Recordset.HasRows) {
	if (!ApplicationStatus_List.Connection.SelectOffset) { // DN
		for (int i = 1; i <= ApplicationStatus_List.StartRecord - 1; i++) { // Move to first record
			if (await ApplicationStatus_List.Recordset.ReadAsync()) {
				ApplicationStatus_List.RecordCount++;
			}
		}
	} else {
		ApplicationStatus_List.RecordCount = ApplicationStatus_List.StartRecord - 1;
	}
} else if (!ApplicationStatus_List.AllowAddDeleteRow && ApplicationStatus_List.StopRecord == 0) {
	ApplicationStatus_List.StopRecord = ApplicationStatus_List.GridAddRowCount;
}

// Initialize aggregate
ApplicationStatus.RowType = Config.RowTypeAggregateInit;
ApplicationStatus_List.ResetAttributes();
await ApplicationStatus_List.RenderRow();

// Output data rows
bool eof = (ApplicationStatus_List.Recordset == null); // DN
while (ApplicationStatus_List.RecordCount < ApplicationStatus_List.StopRecord) { // Begin While
	if (!ApplicationStatus_List.IsGridAdd && !eof) { // DN
		eof = !await ApplicationStatus_List.Recordset.ReadAsync();
	}
	ApplicationStatus_List.RecordCount++;
	if (ApplicationStatus_List.RecordCount >= ApplicationStatus_List.StartRecord) {
		ApplicationStatus_List.RowCount++;

		// Set up key count
		ApplicationStatus_List.KeyCount = ConvertToInt(ApplicationStatus_List.RowIndex);

		// Init row class and style
		ApplicationStatus_List.ResetAttributes();
		ApplicationStatus_List.CssClass = "";
		if (ApplicationStatus_List.IsGridAdd) {
		} else {
			await ApplicationStatus_List.LoadRowValues(ApplicationStatus_List.Recordset); // Load row values
		}
		ApplicationStatus.RowType = Config.RowTypeView; // Render view

		// Set up row id / data-rowindex
		ApplicationStatus_List.RowAttrs.Add("data-rowindex", Convert.ToString(ApplicationStatus_List.RowCount));
		ApplicationStatus_List.RowAttrs.Add("id", "r" + Convert.ToString(ApplicationStatus_List.RowCount) + "_ApplicationStatus");
		ApplicationStatus_List.RowAttrs.Add("data-rowtype", Convert.ToString(ApplicationStatus_List.RowType));

		// Render row
		await ApplicationStatus_List.RenderRow();

		// Render list options
		await ApplicationStatus_List.RenderListOptions();
	@:<tr @Html.Raw(ApplicationStatus.RowAttributes)>
	@* Render list options (body, left) *@
	@ApplicationStatus_List.ListOptions.RenderBody("left", ApplicationStatus_List.RowCount)
	if (ApplicationStatus_List.Id.Visible) { // ApplicationStatus_List.Id.Visible (begin)
		<td data-name="Id" @Html.Raw(ApplicationStatus_List.Id.CellAttributes)>
<span id="el@(ApplicationStatus_List.RowCount)_ApplicationStatus_Id">
<span @Html.Raw(ApplicationStatus_List.Id.ViewAttributes)>@Html.Raw(ApplicationStatus_List.Id.GetViewValue())</span>
</span>
</td>
	} @* ApplicationStatus_List.Id.Visible (end) *@
	if (ApplicationStatus_List.DisplayName.Visible) { // ApplicationStatus_List.DisplayName.Visible (begin)
		<td data-name="DisplayName" @Html.Raw(ApplicationStatus_List.DisplayName.CellAttributes)>
<span id="el@(ApplicationStatus_List.RowCount)_ApplicationStatus_DisplayName">
<span @Html.Raw(ApplicationStatus_List.DisplayName.ViewAttributes)>@Html.Raw(ApplicationStatus_List.DisplayName.GetViewValue())</span>
</span>
</td>
	} @* ApplicationStatus_List.DisplayName.Visible (end) *@
	@* Render list options (body, right) *@
	@ApplicationStatus_List.ListOptions.RenderBody("right", ApplicationStatus_List.RowCount)
	@:</tr>
	} @* End if *@
} @* End While *@
} @* End Code *@
</tbody>
</table><!-- /.ew-table -->
