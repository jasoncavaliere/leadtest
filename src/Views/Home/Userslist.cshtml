@{

	// View
	View = this;
	SetClientVar("login", LoginStatus);

	// Global Page Rendering event
	Page_Rendering();

	// Page Render event
	Users_List.Page_Render();

	// Check action result
	if (Users_List.ActionResult is EmptyResult) { // Terminate page
		return;
	}

	// Layout
	Layout = "_Layout";

	// Local variables (Temp data)
	#pragma warning disable 168
	string sqlWrk, filterWrk, whereWrk, curVal;
	List<Dictionary<string, object>> rswrk;
	string[] arWrk;
	List<object> listWrk;
	Func<string> lookupFilter;
	#pragma warning restore 168
}
@if (!Users_List.IsExport()) {
<script>
var fUserslist, currentPageID;
loadjs.ready("head", function() {

	// Form object
	currentPageID = ew.PAGE_ID = "list";
	fUserslist = currentForm = new ew.Form("fUserslist", "list");
	fUserslist.formKeyCountName = '@(Users_List.FormKeyCountName)';
	loadjs.done("fUserslist");
});
var fUserslistsrch;
loadjs.ready("head", function() {

	// Form object for search
	fUserslistsrch = currentSearchForm = new ew.Form("fUserslistsrch");

	// Dynamic selection lists
	// Filters

	fUserslistsrch.filterList = @Html.Raw(await Users_List.GetFilterList());
	loadjs.done("fUserslistsrch");
});
</script>
<script>
loadjs.ready("head", function() {

	// Client script
	// Write your client script here, no need to add script tags.

});
</script>
}
@if (!Users_List.IsExport()) {
<div class="btn-toolbar ew-toolbar">
@if (Users_List.TotalRecords > 0 && Users_List.ExportOptions.Visible) {
	Users_List.ExportOptions.Render("body");
}
@if (Users_List.ImportOptions.Visible) {
	Users_List.ImportOptions.Render("body");
}
@if (Users_List.SearchOptions.Visible) {
	Users_List.SearchOptions.Render("body");
}
@if (Users_List.FilterOptions.Visible) {
	Users_List.FilterOptions.Render("body");
}
<div class="clearfix"></div>
</div>
}
@{ Users_List.RenderOtherOptions(); }
@if (!Users_List.IsExport() && Empty(Users.CurrentAction)) { @* Show search form (begin) *@
<form name="fUserslistsrch" id="fUserslistsrch" class="form-inline ew-form ew-ext-search-form" action="@AppPath(CurrentPageName())">
<div id="fUserslistsrch-search-panel" class="@(Users_List.SearchPanelClass)"><!-- .ew-search-panel -->
<input type="hidden" name="cmd" value="search">
	<div class="ew-extended-search">
<div id="xsr_@(Users_List.SearchRowCount + 1)" class="ew-row d-sm-flex">
	<div class="ew-quick-search input-group">
		<input type="text" name="@(Config.TableBasicSearch)" id="@(Config.TableBasicSearch)" class="form-control" value="@Html.Raw(HtmlEncode(Users_List.BasicSearch.SessionKeyword))" placeholder="@(HtmlEncode(Language.Phrase("Search")))">
		<input type="hidden" name="@(Config.TableBasicSearchType)" id="@(Config.TableBasicSearchType)" value="@(HtmlEncode(Users_List.BasicSearch.SessionType))">
		<div class="input-group-append">
			<button class="btn btn-primary" name="btn-submit" id="btn-submit" type="submit">@Html.Raw(Language.Phrase("SearchBtn"))</button>
			<button type="button" data-toggle="dropdown" class="btn btn-primary dropdown-toggle dropdown-toggle-split" aria-haspopup="true" aria-expanded="false"><span id="searchtype">@Html.Raw(Users_List.BasicSearch.TypeNameShort)</span></button>
			<div class="dropdown-menu dropdown-menu-right">
				<a class="dropdown-item@((Users_List.BasicSearch.SessionType == "") ? " active" : "")" href="#" onclick="return ew.setSearchType(this);">@Html.Raw(Language.Phrase("QuickSearchAuto"))</a>
				<a class="dropdown-item@((Users_List.BasicSearch.SessionType == "=") ? " active" : "")" href="#" onclick="return ew.setSearchType(this, '=');">@Html.Raw(Language.Phrase("QuickSearchExact"))</a>
				<a class="dropdown-item@((Users_List.BasicSearch.SessionType == "AND") ? " active" : "")" href="#" onclick="return ew.setSearchType(this, 'AND');">@Html.Raw(Language.Phrase("QuickSearchAll"))</a>
				<a class="dropdown-item@((Users_List.BasicSearch.SessionType == "OR") ? " active" : "")" href="#" onclick="return ew.setSearchType(this, 'OR');">@Html.Raw(Language.Phrase("QuickSearchAny"))</a>
			</div>
		</div>
	</div>
</div>
	</div><!-- /.ew-extended-search -->
</div><!-- /.ew-search-panel -->
</form>
} @* Show search form (end) *@
@Users_List.ShowPageHeader()
@Users_List.ShowMessages()
@if (Users_List.TotalRecords > 0 || !Empty(Users.CurrentAction)) {
<div class="card ew-card ew-grid@(Users_List.IsAddOrEdit ? " ew-grid-add-edit" : "") Users">
<form name="fUserslist" id="fUserslist" class="form-inline ew-form ew-list-form" action="@AppPath(CurrentPageName())" method="post">
@if (Users_List.CheckToken) {
	@Html.AntiForgeryToken()
}
<div id="gmp_Users" class="@ResponsiveTableClass() card-body ew-grid-middle-panel">
@if (Users_List.TotalRecords > 0 || Users_List.IsGridEdit) {
<table id="tbl_Userslist" class="table ew-table"><!-- .ew-table -->
<thead>
	<tr class="ew-table-header">
@{

	// Header row
	Users.RowType = Config.RowTypeHeader;

	// Render list options
	await Users_List.RenderListOptions();

	// Render list options (header, left)
	Users_List.ListOptions.Render("header", "left");
}
@if (Users_List.Id.Visible) { // Users_List.Id.Visible (begin)
	if (Users_List.SortUrl(Users_List.Id) == "") {
		<th data-name="Id" class="@(Users_List.Id.HeaderCellClass)"><div id="elh_Users_Id" class="Users_Id"><div class="ew-table-header-caption">@Html.Raw(Users_List.Id.Caption)</div></div></th>
	} else {
		<th data-name="Id" class="@(Users_List.Id.HeaderCellClass)"><div class="ew-pointer" onclick="ew.sort(event, '@Html.Raw(Users_List.SortUrl(Users_List.Id))', 1);"><div id="elh_Users_Id" class="Users_Id">
			<div class="ew-table-header-btn"><span class="ew-table-header-caption">@Html.Raw(Users_List.Id.Caption)</span><span class="ew-table-header-sort">@if (Users_List.Id.Sort == "ASC") {<i class="fas fa-sort-up"></i>} else if (Users_List.Id.Sort == "DESC") {<i class="fas fa-sort-down"></i>}</span></div>
		</div></div></th>
	}
} @* Users_List.Id.Visible (end) *@
@if (Users_List._Name.Visible) { // Users_List._Name.Visible (begin)
	if (Users_List.SortUrl(Users_List._Name) == "") {
		<th data-name="_Name" class="@(Users_List._Name.HeaderCellClass)"><div id="elh_Users__Name" class="Users__Name"><div class="ew-table-header-caption">@Html.Raw(Users_List._Name.Caption)</div></div></th>
	} else {
		<th data-name="_Name" class="@(Users_List._Name.HeaderCellClass)"><div class="ew-pointer" onclick="ew.sort(event, '@Html.Raw(Users_List.SortUrl(Users_List._Name))', 1);"><div id="elh_Users__Name" class="Users__Name">
			<div class="ew-table-header-btn"><span class="ew-table-header-caption">@Html.Raw(Users_List._Name.Caption)@Html.Raw(Language.Phrase("SrchLegend"))</span><span class="ew-table-header-sort">@if (Users_List._Name.Sort == "ASC") {<i class="fas fa-sort-up"></i>} else if (Users_List._Name.Sort == "DESC") {<i class="fas fa-sort-down"></i>}</span></div>
		</div></div></th>
	}
} @* Users_List._Name.Visible (end) *@
@{

	// Render list options (header, right)
	Users_List.ListOptions.Render("header", "right");
}
	</tr>
</thead>
<tbody>
@{ // Begin Code
if (Users_List.ExportAll && Users_List.IsExport()) {
	Users_List.StopRecord = Users_List.TotalRecords;
} else {

	// Set the last record to display
	if (Users_List.TotalRecords > Users_List.StartRecord + Users_List.DisplayRecords - 1) {
		Users_List.StopRecord = Users_List.StartRecord + Users_List.DisplayRecords - 1;
	} else {
		Users_List.StopRecord = Users_List.TotalRecords;
	}
}
if (Users_List.Recordset != null && Users_List.Recordset.HasRows) {
	if (!Users_List.Connection.SelectOffset) { // DN
		for (int i = 1; i <= Users_List.StartRecord - 1; i++) { // Move to first record
			if (await Users_List.Recordset.ReadAsync()) {
				Users_List.RecordCount++;
			}
		}
	} else {
		Users_List.RecordCount = Users_List.StartRecord - 1;
	}
} else if (!Users_List.AllowAddDeleteRow && Users_List.StopRecord == 0) {
	Users_List.StopRecord = Users_List.GridAddRowCount;
}

// Initialize aggregate
Users.RowType = Config.RowTypeAggregateInit;
Users_List.ResetAttributes();
await Users_List.RenderRow();

// Output data rows
bool eof = (Users_List.Recordset == null); // DN
while (Users_List.RecordCount < Users_List.StopRecord) { // Begin While
	if (!Users_List.IsGridAdd && !eof) { // DN
		eof = !await Users_List.Recordset.ReadAsync();
	}
	Users_List.RecordCount++;
	if (Users_List.RecordCount >= Users_List.StartRecord) {
		Users_List.RowCount++;

		// Set up key count
		Users_List.KeyCount = ConvertToInt(Users_List.RowIndex);

		// Init row class and style
		Users_List.ResetAttributes();
		Users_List.CssClass = "";
		if (Users_List.IsGridAdd) {
		} else {
			await Users_List.LoadRowValues(Users_List.Recordset); // Load row values
		}
		Users.RowType = Config.RowTypeView; // Render view

		// Set up row id / data-rowindex
		Users_List.RowAttrs.Add("data-rowindex", Convert.ToString(Users_List.RowCount));
		Users_List.RowAttrs.Add("id", "r" + Convert.ToString(Users_List.RowCount) + "_Users");
		Users_List.RowAttrs.Add("data-rowtype", Convert.ToString(Users_List.RowType));

		// Render row
		await Users_List.RenderRow();

		// Render list options
		await Users_List.RenderListOptions();
	@:<tr @Html.Raw(Users.RowAttributes)>
	@* Render list options (body, left) *@
	@Users_List.ListOptions.RenderBody("left", Users_List.RowCount)
	if (Users_List.Id.Visible) { // Users_List.Id.Visible (begin)
		<td data-name="Id" @Html.Raw(Users_List.Id.CellAttributes)>
<span id="el@(Users_List.RowCount)_Users_Id">
<span @Html.Raw(Users_List.Id.ViewAttributes)>@Html.Raw(Users_List.Id.GetViewValue())</span>
</span>
</td>
	} @* Users_List.Id.Visible (end) *@
	if (Users_List._Name.Visible) { // Users_List._Name.Visible (begin)
		<td data-name="_Name" @Html.Raw(Users_List._Name.CellAttributes)>
<span id="el@(Users_List.RowCount)_Users__Name">
<span @Html.Raw(Users_List._Name.ViewAttributes)>@Html.Raw(Users_List._Name.GetViewValue())</span>
</span>
</td>
	} @* Users_List._Name.Visible (end) *@
	@* Render list options (body, right) *@
	@Users_List.ListOptions.RenderBody("right", Users_List.RowCount)
	@:</tr>
	} @* End if *@
} @* End While *@
} @* End Code *@
</tbody>
</table><!-- /.ew-table -->
} @* End If TotalRecords > 0 *@
</div><!-- /.ew-grid-middle-panel -->
@if (Empty(Users.CurrentAction)) {
<input type="hidden" name="action" id="action" value="">
}
</form><!-- /.ew-list-form -->
@{ Users_List.CloseRecordset(); }
@if (!Users_List.IsExport()) {
<div class="card-footer ew-grid-lower-panel">
@if (!Users_List.IsGridAdd) {
<form name="ew-pager-form" class="form-inline ew-form ew-pager-form" action="@AppPath(CurrentPageName())">
@Html.Raw(Users_List.Pager.Render())
</form>
}
<div class="ew-list-other-options">
@Users_List.OtherOptions.RenderBody("bottom")
</div>
<div class="clearfix"></div>
</div>
}
</div><!-- /.ew-grid -->
}
@if (Users_List.TotalRecords <= 0 && Empty(Users.CurrentAction)) { // Show other options
<div class="ew-list-other-options">
@Users_List.OtherOptions.ForEach(opt => opt.ButtonClass = "").RenderBody()
</div>
<div class="clearfix"></div>
}
@Users_List.ShowPageFooter()
@ShowDebugMessage()
@ElapsedTime()
@if (!Users_List.IsExport()) {
<script>
loadjs.ready("load", function() {

	// Startup script
	// Write your table-specific startup script here
	// document.write("page loaded");

});
</script>
}